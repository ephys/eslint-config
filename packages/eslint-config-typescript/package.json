{
  "name": "@ephys/eslint-config-typescript",
  "version": "21.0.1",
  "description": "ESLint configuration for the foobarHQ JavaScript code style with Typescript support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**",
    "CHANGELOG.md"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "test:eslint": "eslint .",
    "format:eslint": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ephys/eslint-config.git"
  },
  "author": "Zo√© Cox <zoe@ephys.dev>",
  "license": "MIT",
  "homepage": "https://github.com/ephys/eslint-config",
  "dependencies": {
    "@ephys/eslint-config": "^21.0.0",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "typescript-eslint": "^8.53.1"
  }
}
